<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0,  maximum-scale=1.0, user-scalable=no" />
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta http-equiv="cache-control" content="no-cache" />
    <meta http-equiv="pragma" content="no-cache" />
    <meta http-equiv="expires" content="-1" />
    <meta name="robots" content="none">
    <meta name="format-detection" content="telephone=no" />
    <title>@ViewBag.Title</title>
    <link rel="icon" type="image/x-icon" href="favicon.ico" />
    <link rel="shortcut icon" type="image/x-icon" href="favicon.ico" />
    @RenderSection("metasAndLinks", required: false)
    @RenderSection("styles", required: false)

    <environment names="development">
        <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
        <link href="~/lib/css/css-app-bundle.css" rel="stylesheet" />
        <link href="~/lib/css/axa.25faf8b9.css" rel="stylesheet" />
        <link href="~/lib/css/material-icons.css" rel="stylesheet" />
        <link href="~/lib/bootstrap-material-design-icons/css/material-icons.css" rel="stylesheet" />
    </environment>
</head>
<body>
<div class="axaCS-container">
    <section class="axaCS-row">
        <aside class="axaCS-asideNav">
            <Nav class="axaCS-MainNav">
                <div class="axaCS-MainNav-container">
                    <div class="axaCS-MainNav__header">
                        <a class="axaCS-MainNav__header__link" href="#!" title="home">
                            <img class="axaCS-MainNav__header__logo" src="data:image/svg+xml;base64," alt="logo Axa">
                            <h1 class="axaCS-MainNav__header__title"><span>TEST</span>
                            </h1>
                        </a>
                    </div>
                    @if (IsSectionDefined("mainNav"))
                    {
                        <ul class="axaCS-MainNav__lists">
                            @RenderSection("mainNav", required: false)
                        </ul>
                    }
                </div>
            </Nav>
        </aside>
        <div class="axaCS-content">

            @* =========== Header =============== *@

            <header class="axaCS-header">
                <div class="header axaCS-headerContainer">
                    <div class="axaCS-header__navLeft">
                        <div class="axaCS-header__sidebarToggle">
                            <a class="axaCS-header__sidebarToggleLink" href="#!">
                                <i class="material-icons">menu</i>
                            </a>
                        </div>
                        @if (IsSectionDefined("breadcrumb"))
                        {
                            <ul class="axaCS-header__breadcrumb">
                                @RenderSection("breadcrumb", required: false)
                            </ul>
                        }
                    </div>
                    <div class="axaCS-header__navRight">
                        <nav class="axaCS-header__navTools">
                            <ul class="axaCS-header__navTools__lists">
                                @RenderSection("navtools", required: false)
                                <li class="axaCS-header__navTools__list dropdown">
                                    <a class="axaCS-header__navTools__link" href="#!" data-toggle="dropdown">
                                        <span class="axaCS-header__navTools__linkContent">
                                            <i class="material-icons">account</i><span class="axaCS-header__navTools__text">Account</span><i class="icon-Down_Blue"></i>
                                        </span>
                                    </a>
                                    <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                        <li><a asp-action="LogOut" asp-controller="Account" class="dropdown-item">Sign out</a></li>
                                    </ul>
                                </li>
                                <li class="axaCS-header__navTools__list">
                                    <a class="axaCS-helper" href="#!" title="Need help ?" data-toggle="modal" data-target="#exampleModalCenter">
                                        <i class="material-icons">help</i>
                                    </a>
                                </li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </header>

            <main class="axaCS-mainContent">
                @RenderBody()
            </main>
        </div>
        <div class="axaCS-drawer axaCS-drawer--right" id="RightEditor">
        </div>
        <environment names="development">
            <script src="~/lib/bootstrap/dist/js/bootstrap.js" type="text/javascript"></script>
            <script src="~/lib/jQuery/dist/jquery.js" type="text/javascript"></script>
            <script src="~/lib/jQuery/dist/jquery.min.js" type="text/javascript"></script>
            <script src="~/lib/js/app-bundle.js" type="text/javascript"></script>
            <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>
            <script src="~/lib/js/axa-main.34d25469.js"></script>
        </environment>
    </section>
    @RenderSection("helpers", required: false)
    @RenderSection("scripts", required: false)
</div>
</body>
</html>
